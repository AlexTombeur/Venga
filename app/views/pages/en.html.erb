        </section>
        <!-- End Intro Section -->

        <div class="clearfix"></div>

        <!--About Section-->
        <section id="about" class="wow fadeIn ptb ptb-sm-80">
            <div class="container">
                <div class="row text-center">
                    <div class="col-md-8 col-md-offset-2">
                        <h3 class="h4">Venga. Anywhere. Anytime</h3>
                        <div class="vertical-lign"></div>
                        <div class="spacer-15"></div>
                        <p class="lead">Venga is the first universal sport subscription in Brussels. It allows you to experience unique sporting activities offered by certified trainers wherever you are, at your best convenience.</p>

                    </div>
                </div>
            </div>
        </section>
        <!-- End About Section-->


        <section id="how-it-works" class="bg-img1 overlay-dark80 dark-bg ptb ptb-sm-80">
          <div class="container text-center">
            <div class="row">
              <div class="col-md-8 col-md-offset-2">
                <h3>Planning your next workout should be easy</h3>
              </div>
            </div>
            <div class="spacer-60"></div>
            <div class="vertical-lign"></div>
            <div class="spacer-60"></div>
            <div class="row">
              <div class="col-md-4 col-sm-6 mb-45">
              <div class="page-icon-top"><i class="ion ion-ios-search"></i></div>
                <h5>Find your personal trainer</h5>
              </div>
              <div class="col-md-4 col-sm-6 mb-45">
                <div class="page-icon-top"><i class="ion ion-android-done"></i></div>
                <h5>Book your session online</h5>
              </div>
              <div class="col-md-4 col-sm-6 mb-45">
                <div class="page-icon-top"><i class="ion ion-ios-heart-outline"></i></div>
                <h5>Take up the challenge !</h5>
              </div>
            </div>
          </div>
        </section>


