<%= content_for(:after_js) do %>
  <script src="https://maps.google.com/maps/api/js?libraries=places&key=#{ENV['GOOGLE_API_BROWSER_KEY']}"></script>
<% end %>




<% content_for(:after_js) do %>
  <%= javascript_tag do %>
    $('.singlepage-nav').singlePageNav({
            offset: 0,
            filter: ':not(.external)',
            updateHash: true,
            currentClass: 'current',
            easing: 'swing',
            speed: 750,
            beforeStart: function () {
                if ($(window).width() < 991) {
                    $('.singlepage-nav > ul').hide();
                };
            },
            onComplete: function () {
                console.log('done scrolling');
            }
        });
  <% end %>
<% end %>


<% content_for(:js) do %>
  <script>
    $(document).ready(function() {


      $('#btn-index-search-update').click(function(){
        var input = $('#trip_city').val()
        window.location.href = '/trips/search?trip%5Bcity%5D=' + input;
      });


    });
  </script>
<% end %>

